config_version=5

[application]

config/name="medieval-conquerors"
run/main_scene="res://Views/Main/Game.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="medieval-conquerors"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

theme/default_font_antialiasing=2
theme/default_font_generate_mipmaps=true
theme/lcd_subpixel_layout=3
